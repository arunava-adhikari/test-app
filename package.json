{
  "name": "vpn-geoblocking-tester",
  "version": "1.0.0",
  "description": "Automated VPN geo-blocking testing with headless browser",
  "main": "test-vpn-automation.js",
  "scripts": {
    "test": "node run-vpn-test.js",
    "test-basic": "node run-vpn-test.js --config basic",
    "test-demo": "node run-vpn-test.js --config demo",
    "test-comprehensive": "node run-vpn-test.js --config comprehensive",
    "test-headless": "node run-vpn-test.js --config basic --headless",
    "test-compliance": "node run-vpn-test.js --config compliance",
    "test-ci": "node run-vpn-test.js --config ci --yes",
    "install-deps": "npm install puppeteer",
    "setup": "node -e \"console.log('Run: .\\\\setup-integrated-vpn-testing.ps1')\"",
    "demo": "node run-vpn-test.js --config demo"
  },
  "dependencies": {
    "puppeteer": "^21.0.0"
  },
  "keywords": [
    "vpn",
    "geoblocking",
    "testing",
    "automation",
    "puppeteer",
    "headless"
  ],
  "author": "Geo-blocking API Team",
  "license": "MIT"
}
